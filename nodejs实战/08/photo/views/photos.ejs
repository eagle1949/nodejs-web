<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1>Photos</h1>
    <p>express stock photo application</p>
    <div id="photos">
    	<% photos.forEach(function(photo){ %>
			<div class="photo">
				<h2><%=photo.name%></h2>
				<img src='<%=photo.path%>'/>
			</div>
    	<% }) %>
    </div>
  </body>
</html>
